    "@babel/preset-typescript": "^7.13.0",
    "np": "^7.5.0",
    "prettier-standard": "16.4.1",
    "smoothscroll-polyfill": "^0.4.4",
    "stimulus": "^2.0.0",
    "typescript": "^4.2.4",
    "vite": "^2.3.3"
  },
  "peerDependencies": {
    "stimulus": "^2.0.0"
